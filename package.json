{
  "name": "nicht",
  "description": "Command line interface prompt bool values",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:bukinoshita/nicht.git",
  "author": "Bu Kinoshita <bukinoshita@gmail.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "test": "xo --quiet"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "ora": "^1.2.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.1.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.0",
    "prettier": "^1.4.4",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": [
      "prettier"
    ],
    "space": true,
    "semicolon": false,
    "esnext": true,
    "rules": {
      "no-use-before-define": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run test",
      "prettier --semi false --single-quote --write",
      "git add"
    ]
  }
}
